    strm_f2g_isr.tile_6->enable = strm_f2g_ier.tile_6;
    strm_g2f_isr.tile_6->enable = strm_g2f_ier.tile_6;
    par_cfg_g2f_isr.tile_6->enable = par_cfg_g2f_ier.tile_6;
    
    strm_f2g_isr.tile_7->enable = strm_f2g_ier.tile_7;
