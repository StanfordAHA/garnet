    global_isr_r            global_isr;

    // connect interrupt enable to interrupt status register
    
    strm_f2g_isr.tile_0->enable = strm_f2g_ier.tile_0;
    strm_g2f_isr.tile_0->enable = strm_g2f_ier.tile_0;
